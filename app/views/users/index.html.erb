<% provide(:title, 'All users') %>
<h1>All users</h1>

<ul class="users">
  <% @users.each do |user| %>
      <li>
        <%= gravatar_for user%>
        <%= link_to user.name, user %>
      </li>
  <% end %>
  <!--Создаем новых пользователей-->
  <%= link_to "Создать нового пользователя", signup_path %>
</ul>